//! > Test range

//! > test_runner_name
test_expr_semantics(expect_diagnostics: false)

//! > function_body

//! > expr_code
1..10_u8

//! > module_code
use core::iter::RangeOpImpl;

//! > expected_semantics
FunctionCall(
    ExprFunctionCall {
        function: core::iter::traits::iterator::RangeOpImpl::<core::integer::u8, core::integer::U8PartialOrd, core::integer::u8Copy, core::integer::u8Drop>::range,
        args: [
            Value(
                Literal(
                    ExprLiteral {
                        value: 1,
                        ty: core::integer::u8,
                    },
                ),
            ),
            Value(
                Literal(
                    ExprLiteral {
                        value: 10,
                        ty: core::integer::u8,
                    },
                ),
            ),
        ],
        coupon_arg: None,
        ty: core::iter::traits::iterator::Range::<core::integer::u8>,
    },
)

//! > expected_diagnostics
